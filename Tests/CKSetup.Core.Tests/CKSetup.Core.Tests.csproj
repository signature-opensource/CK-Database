<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
  </PropertyGroup>
  
  <Import Project="..\..\Common\Shared.props" />

  <ItemGroup>
    <PackageReference Include="NUnit" Version="[2.6.4]" />
    <PackageReference Include="FluentAssertions" Version="4.19.4" />
    <ProjectReference Include="..\..\CKSetup.Core\CKSetup.Core.csproj" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net461' ">
    <Reference Include="System.Configuration" />
    <Reference Include="System.IO.Compression" />
  </ItemGroup>
  
  <ItemGroup>
    <Compile Include="..\..\CK.DB.Tests.NUnit\AppSettings.cs">
      <Link>Includes\%(RecursiveDir)%(Filename)%(Extension)</Link>
      <Visible>true</Visible>
    </Compile>
  </ItemGroup>
  
  <ItemGroup>
    <Folder Include="CapturedStores\" />
    <Folder Include="TestOutput\" />
  </ItemGroup>



</Project>
