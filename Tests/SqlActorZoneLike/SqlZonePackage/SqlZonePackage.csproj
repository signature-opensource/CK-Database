        in the AssemblyInfo.cs.
    -->
    <PackageReference Include="CK.SqlServer.Dapper" Version="12.0.1--0001-develop" />
    <ProjectReference Include="..\SqlActorPackage\SqlActorPackage.csproj" />
  </ItemGroup>
</Project>