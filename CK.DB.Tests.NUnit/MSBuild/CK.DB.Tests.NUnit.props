  <Project>
    <!--
    This defines the ITestAction NUnit assembly attribute SqlServerConfigurationAspect that configures the SharedEngine configuration.

    Its full name is added to the [assembly: CK.Testing.NUnit.TestHelperMonitorSupport] generated by CK.Testing.NUnit.targets:
    the $(IntermediateOutputPath)CK.Testing.NUnit.AutoAttributes.g.cs file now contains:
    [assembly: CK.Testing.NUnit.TestHelperMonitorSupport, CK.DB.Tests.NUnit.SqlServerConfigurationAspect]

    -->
    <PropertyGroup>
      <CKTestingNUnit>$(CKTestingNUnit), CK.DB.Tests.NUnit.SqlServerConfigurationAspect</CKTestingNUnit>
    </PropertyGroup>
  </Project>
